#c++14 is specified here, because tensorflow doesn't support version >c++14,
# globally specify c++14 but building LambdaServer and other componments with c++2a
build --cxxopt='-std=c++2a'
build --cxxopt=-DHAVE_ZLIB=1
build --define with_glog=true

#build --cxxopt=-D_GLIBCXX_USE_CXX11_ABI=0
build --copt=-"O2"
build --cxxopt='-g' -c dbg
#build --define with_s3=true --define with_ads_cloud=true --cxxopt=-DNOT_USE_KAFKA
build --define with_s3=true --define with_ads_cloud=true --cxxopt=-DNOT_USE_KAFKA
